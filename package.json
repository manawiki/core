{
   "private": true,
   "sideEffects": false,
   "scripts": {
      "build": "yarn run generate:types && remix build && run-s \"build:*\"",
      "build:payload": "payload build",
      "build:remix": "remix build",
      "build:server": "tsc -p tsconfig.server.json",
      "start:core": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js APP_TYPE=core NODE_ENV=production node dist/server.js",
      "dev": "yarn run generate:types && remix build && run-p \"dev:*\"",
      "dev:node": "cross-env NODE_ENV=development APP_TYPE=core nodemon --exec 'ts-node --require dotenv/config ./server.ts'  --watch ./server.ts",
      "dev:remix": "remix watch",
      "custom": "cross-env NODE_ENV=development APP_TYPE=custom PAYLOAD_CONFIG_PATH=payload.custom.config.ts nodemon --exec 'ts-node --require dotenv/config ./server.ts'  --watch ./server.ts",
      "custom-build": "cross-env PAYLOAD_CONFIG_PATH=payload.custom.config.ts payload generate:types && run-s \"custom-build:*\"",
      "custom-build:payload": "cross-env PAYLOAD_CONFIG_PATH=payload.custom.config.ts payload build",
      "custom-build:server": "tsc -p tsconfig.custom.json",
      "start:custom": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.custom.config.js APP_TYPE=custom NODE_ENV=production node dist/server.js",
      "typecheck": "tsc -p tsconfig.server.json",
      "generate:types": "cross-env payload generate:types",
      "generate:types:custom": "cross-env PAYLOAD_CONFIG_PATH=payload.custom.config.ts payload generate:types",
      "clean": "shx rm -rf node_modules distc dist build public/build",
      "seed:core": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts ts-node -T app/_custom/seed/index.ts",
      "seed:custom": "cross-env PAYLOAD_CONFIG_PATH=payload.custom.config.js ts-node -T app/_custom/seed/index.ts"
   },
