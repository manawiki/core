{
   "private": true,
   "sideEffects": false,
   "scripts": {
      "build": "yarn run generate:types && run-p \"build:*\"",
      "build:remix": "remix build",
      "build:payload": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.config.ts payload build",
      "build:server": "tsc -p tsconfig.server.json",
      "start:core": "cross-env PAYLOAD_CONFIG_PATH=dist/app/db/payload.config.js NODE_ENV=production node dist/core.server.js",
      "dev": "run-s generate:types dev:remix",
      "dev:remix": "remix dev -c \"npm run dev:node\" --manual",
      "dev:node": "cross-env NODE_ENV=development PAYLOAD_CONFIG_PATH=./app/db/payload.config.ts node -r ts-node/register --watch-path ./core.server.ts --watch ./core.server.ts",
      "custom": "cross-env NODE_ENV=development PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts node -r ts-node/register --watch-path ./custom.server.ts  --watch ./custom.server.ts",
      "custom-build": "yarn run generate:types:custom && run-s \"custom-build:*\"",
      "custom-build:payload": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts payload build",
      "custom-build:server": "tsc -p tsconfig.custom.json",
      "start:custom": "cross-env PAYLOAD_CONFIG_PATH=dist/app/db/payload.custom.config.js NODE_ENV=production node dist/custom.server.js",
      "typecheck": "tsc -p tsconfig.server.json",
      "generate:icons": "npx @sly-cli/sly add lucide-icons --overwrite --yes --directory ./public/icons",
      "generate:types": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.config.ts yarn generate:types:clean",
      "generate:types:custom": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts yarn generate:types:clean",
      "generate:types:clean": "payload generate:types && node ./scripts/clean-payload-types.js",
      "clean": "shx rm -rf node_modules dist build public/build",
      "seed:core": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.config.ts ts-node -T app/_custom/seed/index.ts",
      "seed:custom": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts ts-node -T app/_custom/seed/index.ts",
      "resave": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts ts-node -T ./scripts/resaveCollection",
      "postinstall": "patch-package"
   },
