{
   "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "jsx": "react-jsx",
      "baseUrl": ".",
      "paths": {
         "~/*": ["./app/*"],
         "payload/generated-types": ["./app/db/payload-types.ts"],
         "payload/generated-custom-types": ["./app/db/payload-custom-types.ts"]      
      },
   },
   "include": ["core.server.ts", "payload.config.ts"]
}
